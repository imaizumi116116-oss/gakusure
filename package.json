{
  "name": "yawaraban",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "postinstall": "prisma generate",
    "predev": "prisma migrate deploy && prisma generate",
    "prebuild": "prisma generate",
    "pretest": "prisma generate",
    "pretypecheck": "prisma generate",
    "dev": "next dev",
    "build": "next build",
    "vercel-build": "prisma migrate deploy --schema prisma/postgres/schema.prisma && prisma generate --schema prisma/postgres/schema.prisma && next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:generate:pg": "prisma generate --schema prisma/postgres/schema.prisma",
    "prisma:migrate:deploy:pg": "prisma migrate deploy --schema prisma/postgres/schema.prisma",
    "db:seed": "prisma db seed",
    "test": "vitest run",
    "test:e2e": "playwright test"
  },
  "dependencies": {
    "@prisma/client": "^5.21.1",
    "clsx": "^2.1.1",
    "next": "^14.2.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^2.5.2",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.53.2",
    "@types/node": "^20.12.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.5",
    "eslint-config-prettier": "^9.1.0",
    "postcss": "^8.4.41",
    "prettier": "^3.3.3",
    "prisma": "^5.21.1",
    "tailwindcss": "^3.4.10",
    "tsx": "^4.17.0",
    "typescript": "^5.5.4",
    "vitest": "^2.1.4"
  },
  "prisma": {
    "seed": "node --import tsx prisma/seed.ts"
  }
}
